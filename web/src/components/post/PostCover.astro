---
import { SharpPicture } from "@/modules/picture/components";

type Props = Pick<
	component.Props_<typeof SharpPicture>,
	"src"
>

const { src, ...props } = Astro.props
---

<SharpPicture
    src={src}
    alt="Cover"
    width={1920}
    height={1080}
    computeImg={(img) => img.greyscale()}
    {...props}
/>